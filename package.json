    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.3",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0"
  },
  "optionalDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
